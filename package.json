  "author": "luizboldrin <luizboldrin7@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --ignore-watch node_modules src/index.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored",
    "start": "yarn build && node build/index.js",
    "check-types": "tsc --noemit",
    "lint": "yarn run eslint . --ext .js,.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/dotenv": "^8.2.0",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "playwright": "^1.17.1"
  }
}
